<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#1B1919">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/viewzero"
        android:text="Let's check your product"
        android:textAlignment="center"
        android:paddingTop="10dp"
        android:textColor="#ffffff"
        android:background="@drawable/text"
        android:textSize="20dp"
        android:paddingLeft="20dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.030"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"></TextView>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/fotozero"
        app:layout_constraintTop_toTopOf="@id/viewzero"
        app:layout_constraintBottom_toBottomOf="@id/viewzero"
        app:layout_constraintRight_toRightOf="@id/viewzero"
        app:layout_constraintLeft_toLeftOf="@id/viewzero"
        app:layout_constraintVertical_bias="0.030"
        app:layout_constraintHorizontal_bias="0.010"></ImageView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="145dp"
        android:id="@+id/view1"
        android:background="@drawable/background"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toTopOf="@id/viewzero"
        app:layout_constraintBottom_toBottomOf="@id/view2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.370"></TextView>
    <ImageView
        android:id="@+id/image1"
        android:layout_width="140dp"
        android:layout_height="92dp"
        android:src="@drawable/fotopertama"
        app:layout_constraintTop_toTopOf="@id/view1"
        app:layout_constraintRight_toRightOf="@id/view1"
        app:layout_constraintLeft_toLeftOf="@id/view1"
        app:layout_constraintBottom_toBottomOf="@id/view1"
        app:layout_constraintVertical_bias="0.470"
        app:layout_constraintHorizontal_bias="0.0310"></ImageView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cek makanan dan"
        android:textColor="#1B1919"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/view1"
        app:layout_constraintBottom_toBottomOf="@id/view1"
        app:layout_constraintLeft_toLeftOf="@id/image1"
        app:layout_constraintRight_toRightOf="@id/view1"
        app:layout_constraintVertical_bias="0.210"
        app:layout_constraintHorizontal_bias="0.650"></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="minuman"
        android:textColor="#1B1919"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/view1"
        app:layout_constraintBottom_toBottomOf="@id/view1"
        app:layout_constraintLeft_toLeftOf="@id/image1"
        app:layout_constraintRight_toRightOf="@id/view1"
        app:layout_constraintVertical_bias="0.400"
        app:layout_constraintHorizontal_bias="0.489"></TextView>
    <Button
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="Check"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:background="@drawable/button"
        app:layout_constraintTop_toTopOf="@id/view1"
        app:layout_constraintBottom_toBottomOf="@id/view1"
        app:layout_constraintLeft_toLeftOf="@id/view1"
        app:layout_constraintRight_toRightOf="@id/view1"
        app:layout_constraintVertical_bias="0.9"
        app:layout_constraintHorizontal_bias="0.920"></Button>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="145dp"
        android:id="@+id/view2"
        android:background="@drawable/background"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toTopOf="@id/view1"
        app:layout_constraintBottom_toBottomOf="@id/view3"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"></TextView>
    <ImageView
        android:id="@+id/image2"
        android:layout_width="140dp"
        android:layout_height="100dp"
        android:src="@drawable/buah"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintBottom_toBottomOf="@id/view2"
        app:layout_constraintLeft_toLeftOf="@id/view2"
        app:layout_constraintRight_toRightOf="@id/view2"
        app:layout_constraintVertical_bias="0.470"
        app:layout_constraintHorizontal_bias="0.0310"></ImageView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cek Buah dan Sayur"
        android:textColor="#1B1919"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintBottom_toBottomOf="@id/view2"
        app:layout_constraintLeft_toLeftOf="@id/image2"
        app:layout_constraintRight_toRightOf="@id/view2"
        app:layout_constraintVertical_bias="0.360"
        app:layout_constraintHorizontal_bias="0.712"></TextView>
    <Button
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="Check"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:background="@drawable/button"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintBottom_toBottomOf="@id/view2"
        app:layout_constraintLeft_toLeftOf="@id/view2"
        app:layout_constraintRight_toRightOf="@id/view2"
        app:layout_constraintVertical_bias="0.9"
        app:layout_constraintHorizontal_bias="0.920"></Button>
    <TextView
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="145dp"
        android:background="@drawable/background"
        android:layout_marginTop="8dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"></TextView>
    <ImageView
        android:id="@+id/image3"
        android:layout_width="140dp"
        android:layout_height="92dp"
        android:src="@drawable/kardus"
        app:layout_constraintTop_toTopOf="@id/view3"
        app:layout_constraintBottom_toBottomOf="@id/view3"
        app:layout_constraintLeft_toLeftOf="@id/view3"
        app:layout_constraintRight_toRightOf="@id/view3"
        app:layout_constraintVertical_bias="0.470"
        app:layout_constraintHorizontal_bias="0.0310"></ImageView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cek Barang Lainnya"
        android:textStyle="bold"
        android:textSize="20dp"
        android:textColor="#1B1919"
        app:layout_constraintTop_toTopOf="@id/view3"
        app:layout_constraintRight_toRightOf="@id/view3"
        app:layout_constraintLeft_toLeftOf="@id/image3"
        app:layout_constraintBottom_toBottomOf="@id/view3"
        app:layout_constraintVertical_bias="0.340"
        app:layout_constraintHorizontal_bias="0.7"></TextView>
    <Button
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="Check"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:background="@drawable/button"
        app:layout_constraintTop_toTopOf="@id/view3"
        app:layout_constraintBottom_toBottomOf="@id/view3"
        app:layout_constraintLeft_toLeftOf="@id/view3"
        app:layout_constraintRight_toRightOf="@id/view3"
        app:layout_constraintVertical_bias="0.9"
        app:layout_constraintHorizontal_bias="0.920"></Button>
    <TextView
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/end"
        android:drawableStart="@drawable/home"
        android:paddingLeft="7dp"
        android:paddingRight="7dp"
        android:drawableEnd="@drawable/setting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/view3"
        app:layout_constraintVertical_bias="0.995"></TextView>
    <TextView
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginBottom="5dp"
        android:drawableStart="@drawable/notif"
        android:paddingLeft="60dp"
        android:paddingRight="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/view3"
        app:layout_constraintVertical_bias="0.995"></TextView>
</androidx.constraintlayout.widget.ConstraintLayout>